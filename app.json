{
  "expo": {
    "name": "Profesy",
    "slug": "Profesy",
    "version": "1.1.8",
    "orientation": "portrait",
    "icon": "./utils/icon_dark.png",
    "splash": {
      "image": "./utils/icon_dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.elitracy.Profesy"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./utils/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
